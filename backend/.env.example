# CricketIQ Backend Environment Variables
# Copy this file to .env and fill in your values

# AI Provider (gemini is currently the only supported provider)
PROVIDER=gemini

# Gemini API Key - Get yours from https://aistudio.google.com/app/apikey
GEMINI_API_KEY=your-gemini-api-key-here

# MongoDB Connection
MONGO_URI=mongodb://localhost:27017
MONGO_DB=cricketiq
MONGO_COLLECTION_DELIVERYWISE=deliverywise
MONGO_COLLECTION_MATCHWISE=matchwise
